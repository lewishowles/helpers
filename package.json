{
	"name": "helpers",
	"version": "0.0.1",
	"description": "A library of gorgeous Javascript methods to make working with—and validating—data quicker and less error-prone, letting the developer concentrate on the fun stuff.",
	"author": "Lewis Howles",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/helpers.umd.js",
	"module": "./dist/helpers.es.js",
	"exports": {
		"array": {
			"import": "./dist/array.es.js",
			"require": "./dist/array.umd.js"
		},
		"general": {
			"import": "./dist/general.es.js",
			"require": "./dist/general.umd.js"
		},
		"number": {
			"import": "./dist/number.es.js",
			"require": "./dist/number.umd.js"
		},
		"object": {
			"import": "./dist/object.es.js",
			"require": "./dist/object.umd.js"
		},
		"string": {
			"import": "./dist/string.es.js",
			"require": "./dist/string.umd.js"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest",
		"lint": "eslint . --ext .js --fix"
	},
	"devDependencies": {
		"@stylistic/eslint-plugin": "^2.6.2",
		"eslint": "^9.9.0",
		"globals": "^15.9.0",
		"vite": "^5.4.0",
		"vitest": "^2.0.5"
	}
}
